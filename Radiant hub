local Section = Tab2:CreateSection("ChainSaw Man")



local Button = Tab2:CreateButton({

   Name = "Chainsaw Man",

   Callback = function()

   --/ run speed \\--

getgenv().RunSpeed = 100



--/ jump power while running \\--

getgenv().RunJump = 100



--/ instant kill (first and second skill) \\--

getgenv().InstaKill = false



--/ chance to revive after dying (100% in this case) \\--

getgenv().RevivePercent = 40



--// walk anim \\\--

getgenv().ChangeWalk = true



--/ idle anim \\--

getgenv().ChangeIdle = true



--/ night time \\--

getgenv().Night = false



--/ side dash no cooldown \\--

getgenv().DashNoCooldown = false



--/ extra skills \\--

getgenv().ExtraSkills = true





loadstring(game:HttpGet("https://raw.githubusercontent.com/yes1nt/yes/refs/heads/main/CHAINSAW%20MAN/Chainsaw%20Man%20(Obfuscated).txt"))()

   end

})



